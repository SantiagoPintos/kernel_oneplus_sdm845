						07 00 00 00
						80 80 80
						80 80 80
						80 80 80
						80 80 80
						80 80 80
						80 80 80
						80 80 80
						80 80 80
						80 80 80
						00 00 00
					29 00 00 00 00 00 03 B1 00 0C   /* AID Set : 0% */
					29 00 00 00 00 00 17	/* MPS/ELVSS Setting */
						B5
						19 DC 16 /* temp=25, MPS=high, ELVSS=420nit */
						01 34 67 9A CD 01 22 33
						44 00 00 05 55 CC 0C 01
						11 11 10
					29 00 00 00 00 00 03 F4 EB 28   /* VINT */
					29 00 00 00 00 00 13		/* IRC */
						B8
						15 20 69 E7 5A 06 57 53 88 59 59 59 1D 1D 1D 1E 1E 1E
					29 01 00 00 00 00 02 F7 03      /* Gamma, AID update  */
					29 01 00 00 00 00 03 F0 A5 A5   /* TEST KEY1 Disable */
					29 01 00 00 00 00 03 9F 5A 5A	/* LV1(9F) key disable  */
					29 01 00 00 00 00 03 9F A5 A5	// TEST KEY Enable
					05 01 00 00 00 00 01 01 	// S/W Reset, Delay 120ms
					29 01 00 00 00 00 03 F0 5A 5A	// TEST KEY Enable
					29 01 00 00 00 00 03 FC 5A 5A	// TEST KEY Enable
					29 01 00 00 00 00 02 B0 0A	// Global Para
					29 01 00 00 00 00 02 FE 14	// Para
					05 01 00 00 0A 00 01 11 	// Sleep out, Delay 10ms
					29 01 00 00 00 00 02 9D 01	// DSC Enable
					29 01 00 00 00 00 5A 9E 	/* PPS */
					11 00 00 89 30 80 0B 90
					05 A0 00 28 02 D0 02 D0
					02 00 02 A4 00 20 04 0B
					00 0A 00 0E 02 E0 01 E9
					18 00 10 F0 03 0C 20 00
					06 0B 0B 33 0E 1C 2A 38
					46 54 62 69 70 77 79 7B
					7D 7E 01 02 01 00 09 40
					09 BE 19 FC 19 FA 19 F8
					1A 38 1A 78 1A B6 2A B6
					2A F4 2A F4 4B 34 63 74
					00
					39 01 00 00 00 00 03 FC A5 A5	
					39 00 00 00 00 00 03 F0 A5 A5
					05 01 00 00 00 00 02 29 00	// display on
					29 01 00 00 00 00 02 B0 09	// Global Para
					29 01 00 00 00 00 02 F4 10	/* VDDM LV Setting (VDDM 0x0: 1.0V, 0x10: 0.9V, 0x30: 1.1V) */
					29 01 00 00 00 00 02 FE B0	// Para
					29 01 00 00 32 00 02 FE 30	// Para, Delay 50ms
					05 01 00 00 00 00 02 2C 00	// Checksum start
					29 01 00 00 00 00 02 BE 07	// GDBIST Pattern 1
					];
				qcom,mdss-dsi-on-command-state = "dsi_lp_mode";
				qcom,mdss-dsi-off-command-state = "dsi_hs_mode";
				qcom,mdss-dsi-h-sync-pulse = <0>;
				
				qcom,mdss-dsi-off-command=[
					05 01 00 00 00 00 02 28 00
					29 01 00 00 00 00 03 9F A5 A5		/* LV1 (9F) unlock */
					05 01 00 00 00 00 02 10 00		/* sleep in */
					29 01 00 00 B4 00 03 9F 5A 5A		/* LV1 (9F) lock, 180ms delay to match 120ms */
